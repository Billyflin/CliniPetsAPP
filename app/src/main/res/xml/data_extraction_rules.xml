<?xml version="1.0" encoding="utf-8"?><!--
   Sample data extraction rules file; uncomment and customize as necessary.
   See https://developer.android.com/about/versions/12/backup-restore#xml-changes
   for details.
-->
<data-extraction-rules>
    <cloud-backup>
        <!-- Excluir preferencias cifradas -->
        <exclude domain="sharedpref" path="secret_shared_prefs.xml"/>
        <!-- Excluir keyset Tink guardado en SharedPreferences -->
        <exclude domain="sharedpref" path="clinipets_aead_keyset_prefs.xml"/>
        <!-- Excluir DataStore sensible de tokens -->
        <exclude domain="file" path="datastore/secure_prefs.preferences_pb"/>
        <exclude domain="file" path="datastore/secure_prefs.preferences_pb-journal"/>
    </cloud-backup>
    <device-transfer>
        <exclude domain="sharedpref" path="secret_shared_prefs.xml"/>
        <exclude domain="sharedpref" path="clinipets_aead_keyset_prefs.xml"/>
        <exclude domain="file" path="datastore/secure_prefs.preferences_pb"/>
        <exclude domain="file" path="datastore/secure_prefs.preferences_pb-journal"/>
    </device-transfer>
</data-extraction-rules>